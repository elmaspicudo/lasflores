{% extends '::base.html.twig' %}

{% block body -%}
    <h1>anadirQueja</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Quejano</th>
                <td>{{ entity.quejaNo }}</td>
            </tr>
            <tr>
                <th>Titulo</th>
                <td>{{ entity.titulo }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Juiciofecha</th>
                <td>{{ entity.juicioFecha|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Motivodelaqueja</th>
                <td>{{ entity.motivoDeLaQueja }}</td>
            </tr>
            <tr>
                <th>Demanda</th>
                <td>{{ entity.demanda }}</td>
            </tr>
            <tr>
                <th>Jurado</th>
                <td>{{ entity.jurado }}</td>
            </tr>
            <tr>
                <th>Autoridadesresponsables</th>
                <td>{{ entity.autoridadesResponsables }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('anadirqueja') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('anadirqueja_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
