{% extends '::base.html.twig' %}

{% block body -%}
    <h1>curso</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombredelcurso</th>
                <td>{{ entity.nombreDelCurso }}</td>
            </tr>
            <tr>
                <th>Nombredelaseccion</th>
                <td>{{ entity.nombreDeLaSeccion }}</td>
            </tr>
            <tr>
