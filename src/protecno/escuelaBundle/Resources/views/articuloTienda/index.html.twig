{% extends '::base.html.twig' %}

{% block body -%}
    <h1>articuloTienda list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombredelelemento</th>
                <th>Codigo</th>
                <th>Cantidad</th>
                <th>Precioporunidad</th>
                <th>Impuestosporcentaje</th>
                <th>Nodelote</th>
                <th>Vendible</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('articulotienda_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.nombreDelElemento }}</td>
                <td>{{ entity.codigo }}</td>
                <td>{{ entity.cantidad }}</td>
                <td>{{ entity.precioPorUnidad }}</td>
                <td>{{ entity.impuestosPorcentaje }}</td>
                <td>{{ entity.noDeLote }}</td>
                <td>{{ entity.vendible }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('articulotienda_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('articulotienda_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('articulotienda_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
