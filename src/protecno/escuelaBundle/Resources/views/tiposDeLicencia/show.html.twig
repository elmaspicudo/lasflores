{% extends '::base.html.twig' %}

{% block body -%}
    <h1>tiposDeLicencia</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Dejanombre</th>
                <td>{{ entity.dejaNombre }}</td>
            </tr>
            <tr>
                <th>Dejacodigo</th>
                <td>{{ entity.dejaCodigo }}</td>
            </tr>
            <tr>
                <th>Elnumeromaximodelicencia</th>
                <td>{{ entity.elNumeroMaximoDeLicencia }}</td>
            </tr>
            <tr>
                <th>Restablecerfecha</th>
                <td>{{ entity.restablecerFecha|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tiposdelicencia') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('tiposdelicencia_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
