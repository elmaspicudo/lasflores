{% extends '::base.html.twig' %}

{% block body -%}
    <h1>tiposDeLicencia list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Dejanombre</th>
                <th>Dejacodigo</th>
                <th>Elnumeromaximodelicencia</th>
                <th>Restablecerfecha</th>
                <th>Estado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('tiposdelicencia_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.dejaNombre }}</td>
                <td>{{ entity.dejaCodigo }}</td>
                <td>{{ entity.elNumeroMaximoDeLicencia }}</td>
                <td>{% if entity.restablecerFecha %}{{ entity.restablecerFecha|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.estado }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('tiposdelicencia_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('tiposdelicencia_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('tiposdelicencia_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
