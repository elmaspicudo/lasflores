{% extends '::base.html.twig' %}

{% block body -%}
    <h1>proveedores</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombredelproveedor</th>
                <td>{{ entity.nombreDelProveedor }}</td>
            </tr>
            <tr>
                <th>Nocontacto</th>
                <td>{{ entity.noContacto }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ entity.direccion }}</td>
            </tr>
            <tr>
                <th>Notin</th>
                <td>{{ entity.noTin }}</td>
            </tr>
            <tr>
                <th>Region</th>
                <td>{{ entity.region }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('proveedores') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('proveedores_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
