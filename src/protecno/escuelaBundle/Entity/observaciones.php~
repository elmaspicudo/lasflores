<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * observaciones
 */
class observaciones
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $tema;

    /**
     * @var string
     */
    private $observacionPor;

    /**
     * @var string
     */
    private $descripcion;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set tema
     *
     * @param string $tema
     * @return observaciones
     */
    public function setTema($tema)
    {
        $this->tema = $tema;
    
        return $this;
    }

    /**
     * Get tema
     *
     * @return string 
     */
    public function getTema()
    {
        return $this->tema;
    }

    /**
     * Set observacionPor
     *
     * @param string $observacionPor
     * @return observaciones
     */
    public function setObservacionPor($observacionPor)
    {
        $this->observacionPor = $observacionPor;
    
        return $this;
    }

    /**
     * Get observacionPor
     *
     * @return string 
     */
    public function getObservacionPor()
    {
        return $this->observacionPor;
    }

    /**
     * Set descripcion
     *
     * @param string $descripcion
     * @return observaciones
     */
    public function setDescripcion($descripcion)
    {
        $this->descripcion = $descripcion;
    
        return $this;
    }

    /**
     * Get descripcion
     *
     * @return string 
     */
    public function getDescripcion()
    {
        return $this->descripcion;
    }
}