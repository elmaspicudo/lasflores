<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * nuevoGNR
 */
class nuevoGNR
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $gNRno;

    /**
     * @var integer
     */
    private $facturaNo;

    /**
     * @var \DateTime
     */
    private $gNRFecha;

    /**
     * @var \DateTime
     */
    private $fechaDeLaFactura;

    /**
     * @var string
     */
    private $otrosCargos;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set gNRno
     *
     * @param integer $gNRno
     * @return nuevoGNR
     */
    public function setGNRno($gNRno)
    {
        $this->gNRno = $gNRno;
    
        return $this;
    }

    /**
     * Get gNRno
     *
     * @return integer 
     */
    public function getGNRno()
    {
        return $this->gNRno;
    }

    /**
     * Set facturaNo
     *
     * @param integer $facturaNo
     * @return nuevoGNR
     */
    public function setFacturaNo($facturaNo)
    {
        $this->facturaNo = $facturaNo;
    
        return $this;
    }

    /**
     * Get facturaNo
     *
     * @return integer 
     */
    public function getFacturaNo()
    {
        return $this->facturaNo;
    }

    /**
     * Set gNRFecha
     *
     * @param \DateTime $gNRFecha
     * @return nuevoGNR
     */
    public function setGNRFecha($gNRFecha)
    {
        $this->gNRFecha = $gNRFecha;
    
        return $this;
    }

    /**
     * Get gNRFecha
     *
     * @return \DateTime 
     */
    public function getGNRFecha()
    {
        return $this->gNRFecha;
    }

    /**
     * Set fechaDeLaFactura
     *
     * @param \DateTime $fechaDeLaFactura
     * @return nuevoGNR
     */
    public function setFechaDeLaFactura($fechaDeLaFactura)
    {
        $this->fechaDeLaFactura = $fechaDeLaFactura;
    
        return $this;
    }

    /**
     * Get fechaDeLaFactura
     *
     * @return \DateTime 
     */
    public function getFechaDeLaFactura()
    {
        return $this->fechaDeLaFactura;
    }

    /**
     * Set otrosCargos
     *
     * @param string $otrosCargos
     * @return nuevoGNR
     */
    public function setOtrosCargos($otrosCargos)
    {
        $this->otrosCargos = $otrosCargos;
    
        return $this;
    }

    /**
     * Get otrosCargos
     *
     * @return string 
     */
    public function getOtrosCargos()
    {
        return $this->otrosCargos;
    }
}