<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * crearReglaDeReembolso
 */
class crearReglaDeReembolso
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $reembolsoNombre;

    /**
     * @var \DateTime
     */
    private $reembolsoValides;

    /**
     * @var boolean
     */
    private $tipo;

    /**
     * @var integer
     */
    private $valor;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set reembolsoNombre
     *
     * @param string $reembolsoNombre
     * @return crearReglaDeReembolso
     */
    public function setReembolsoNombre($reembolsoNombre)
    {
        $this->reembolsoNombre = $reembolsoNombre;
    
        return $this;
    }

    /**
     * Get reembolsoNombre
     *
     * @return string 
     */
    public function getReembolsoNombre()
    {
        return $this->reembolsoNombre;
    }

    /**
     * Set reembolsoValides
     *
     * @param \DateTime $reembolsoValides
     * @return crearReglaDeReembolso
     */
    public function setReembolsoValides($reembolsoValides)
    {
        $this->reembolsoValides = $reembolsoValides;
    
        return $this;
    }

    /**
     * Get reembolsoValides
     *
     * @return \DateTime 
     */
    public function getReembolsoValides()
    {
        return $this->reembolsoValides;
    }

    /**
     * Set tipo
     *
     * @param boolean $tipo
     * @return crearReglaDeReembolso
     */
    public function setTipo($tipo)
    {
        $this->tipo = $tipo;
    
        return $this;
    }

    /**
     * Get tipo
     *
     * @return boolean 
     */
    public function getTipo()
    {
        return $this->tipo;
    }

    /**
     * Set valor
     *
     * @param integer $valor
     * @return crearReglaDeReembolso
     */
    public function setValor($valor)
    {
        $this->valor = $valor;
    
        return $this;
    }

    /**
     * Get valor
     *
     * @return integer 
     */
    public function getValor()
    {
        return $this->valor;
    }
}