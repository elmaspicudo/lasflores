<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * lotes
 */
class lotes
{
    /**
     * @var integer
     */
    private $id;
    
    /**
     * @var string
     */
    private $descripcion;
    
    /**
     * @var \DateTime
     */
    private $fechaDeIncio;

    /**
     * @var \DateTime
     */
    private $fechaDeFinal;

    /**
     * Get id
     *
     * @return integer 
     */
    public function __toString()
    {
        return $this->descripcion;
    }

    /**
     * Set descripcion
     *
     * @param string $descripcion
     * @return lotes
     */
    public function setDescripcion($descripcion)
    {
        $this->descripcion = $descripcion;
    
        return $this;
    }

    /**
     * Get descripcion
     *
     * @return string 
     */
    public function getDescripcion()
    {
        return $this->descripcion;
    }

    /**
     * Set fechaDeIncio
     *
     * @param \DateTime $fechaDeIncio
     * @return lotes
     */
    public function setFechaDeIncio($fechaDeIncio)
    {
        $this->fechaDeIncio = $fechaDeIncio;
    
        return $this;
    }

    /**
     * Get fechaDeIncio
     *
     * @return \DateTime 
     */
    public function getFechaDeIncio()
    {
        return $this->fechaDeIncio;
    }

    /**
     * Set fechaDeFinal
     *
     * @param \DateTime $fechaDeFinal
     * @return lotes
     */
    public function setFechaDeFinal($fechaDeFinal)
    {
        $this->fechaDeFinal = $fechaDeFinal;
    
        return $this;
    }

    /**
     * Get fechaDeFinal
     *
     * @return \DateTime 
     */
    public function getFechaDeFinal()
    {
        return $this->fechaDeFinal;
    }
        /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}