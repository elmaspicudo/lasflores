<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * categoriasDeEstudiante
 */
class categoriasDeEstudiante
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $ingreseNuevaRuta;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set ingreseNuevaRuta
     *
     * @param string $ingreseNuevaRuta
     * @return categoriasDeEstudiante
     */
    public function setIngreseNuevaRuta($ingreseNuevaRuta)
    {
        $this->ingreseNuevaRuta = $ingreseNuevaRuta;
    
        return $this;
    }

    /**
     * Get ingreseNuevaRuta
     *
     * @return string 
     */
    public function getIngreseNuevaRuta()
    {
        return $this->ingreseNuevaRuta;
    }
}