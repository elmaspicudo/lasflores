<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * nuevoCliente
 */
class nuevoCliente
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $nombreDelCliente;

    /**
     * @var string
     */
    private $redirigidaURL;

    /**
     * @var boolean
     */
    private $verificado;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nombreDelCliente
     *
     * @param string $nombreDelCliente
     * @return nuevoCliente
     */
    public function setNombreDelCliente($nombreDelCliente)
    {
        $this->nombreDelCliente = $nombreDelCliente;
    
        return $this;
    }

    /**
     * Get nombreDelCliente
     *
     * @return string 
     */
    public function getNombreDelCliente()
    {
        return $this->nombreDelCliente;
    }

    /**
     * Set redirigidaURL
     *
     * @param string $redirigidaURL
     * @return nuevoCliente
     */
    public function setRedirigidaURL($redirigidaURL)
    {
        $this->redirigidaURL = $redirigidaURL;
    
        return $this;
    }

    /**
     * Get redirigidaURL
     *
     * @return string 
     */
    public function getRedirigidaURL()
    {
        return $this->redirigidaURL;
    }

    /**
     * Set verificado
     *
     * @param boolean $verificado
     * @return nuevoCliente
     */
    public function setVerificado($verificado)
    {
        $this->verificado = $verificado;
    
        return $this;
    }

    /**
     * Get verificado
     *
     * @return boolean 
     */
    public function getVerificado()
    {
        return $this->verificado;
    }
}