<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * honorariosInstantaneos
 */
class honorariosInstantaneos
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $fields;

    /**
     * @var boolean
     */
    private $estudiante;

    /**
     * @var boolean
     */
    private $empleado;

    /**
     * @var boolean
     */
    private $invitado;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set fields
     *
     * @param string $fields
     * @return honorariosInstantaneos
     */
    public function setFields($fields)
    {
        $this->fields = $fields;
    
        return $this;
    }

    /**
     * Get fields
     *
     * @return string 
     */
    public function getFields()
    {
        return $this->fields;
    }

    /**
     * Set estudiante
     *
     * @param boolean $estudiante
     * @return honorariosInstantaneos
     */
    public function setEstudiante($estudiante)
    {
        $this->estudiante = $estudiante;
    
        return $this;
    }

    /**
     * Get estudiante
     *
     * @return boolean 
     */
    public function getEstudiante()
    {
        return $this->estudiante;
    }

    /**
     * Set empleado
     *
     * @param boolean $empleado
     * @return honorariosInstantaneos
     */
    public function setEmpleado($empleado)
    {
        $this->empleado = $empleado;
    
        return $this;
    }

    /**
     * Get empleado
     *
     * @return boolean 
     */
    public function getEmpleado()
    {
        return $this->empleado;
    }

    /**
     * Set invitado
     *
     * @param boolean $invitado
     * @return honorariosInstantaneos
     */
    public function setInvitado($invitado)
    {
        $this->invitado = $invitado;
    
        return $this;
    }

    /**
     * Get invitado
     *
     * @return boolean 
     */
    public function getInvitado()
    {
        return $this->invitado;
    }
}