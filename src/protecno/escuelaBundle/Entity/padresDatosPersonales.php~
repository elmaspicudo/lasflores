    /**
     * @var string
     */
    private $ocupacion;

    /**
     * @var integer
     */
    private $ingresos;

    /**
     * @var string
     */
    private $eMail;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

