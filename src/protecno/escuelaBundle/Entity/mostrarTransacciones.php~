<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * mostrarTransacciones
 */
class mostrarTransacciones
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var \DateTime
     */
    private $seleccionarFechaDeInicio;

    /**
     * @var \DateTime
     */
    private $seleccioneFechaDeFinalizacion;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set seleccionarFechaDeInicio
     *
     * @param \DateTime $seleccionarFechaDeInicio
     * @return mostrarTransacciones
     */
    public function setSeleccionarFechaDeInicio($seleccionarFechaDeInicio)
    {
        $this->seleccionarFechaDeInicio = $seleccionarFechaDeInicio;
    
        return $this;
    }

    /**
     * Get seleccionarFechaDeInicio
     *
     * @return \DateTime 
     */
    public function getSeleccionarFechaDeInicio()
    {
        return $this->seleccionarFechaDeInicio;
    }

    /**
     * Set seleccioneFechaDeFinalizacion
     *
     * @param \DateTime $seleccioneFechaDeFinalizacion
     * @return mostrarTransacciones
     */
    public function setSeleccioneFechaDeFinalizacion($seleccioneFechaDeFinalizacion)
    {
        $this->seleccioneFechaDeFinalizacion = $seleccioneFechaDeFinalizacion;
    
        return $this;
    }

    /**
     * Get seleccioneFechaDeFinalizacion
     *
     * @return \DateTime 
     */
    public function getSeleccioneFechaDeFinalizacion()
    {
        return $this->seleccioneFechaDeFinalizacion;
    }
}