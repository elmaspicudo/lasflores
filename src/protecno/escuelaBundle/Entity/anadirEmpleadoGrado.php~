<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * anadirEmpleadoGrado
 */
class anadirEmpleadoGrado
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $nombre;

    /**
     * @var string
     */
    private $prioridad;

    /**
     * @var string
     */
    private $numeroMaximoDeHorasPorDia;

    /**
     * @var string
     */
    private $numeroMaximoDeHorasPorSemana;

    /**
     * @var boolean
     */
    private $estado;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nombre
     *
     * @param string $nombre
     * @return anadirEmpleadoGrado
     */
    public function setNombre($nombre)
    {
        $this->nombre = $nombre;
    
        return $this;
    }

    /**
     * Get nombre
     *
     * @return string 
     */
    public function getNombre()
    {
        return $this->nombre;
    }

    /**
     * Set prioridad
     *
     * @param string $prioridad
     * @return anadirEmpleadoGrado
     */
    public function setPrioridad($prioridad)
    {
        $this->prioridad = $prioridad;
    
        return $this;
    }

    /**
     * Get prioridad
     *
     * @return string 
     */
    public function getPrioridad()
    {
        return $this->prioridad;
    }

    /**
     * Set numeroMaximoDeHorasPorDia
     *
     * @param string $numeroMaximoDeHorasPorDia
     * @return anadirEmpleadoGrado
     */
    public function setNumeroMaximoDeHorasPorDia($numeroMaximoDeHorasPorDia)
    {
        $this->numeroMaximoDeHorasPorDia = $numeroMaximoDeHorasPorDia;
    
        return $this;
    }

    /**
     * Get numeroMaximoDeHorasPorDia
     *
     * @return string 
     */
    public function getNumeroMaximoDeHorasPorDia()
    {
        return $this->numeroMaximoDeHorasPorDia;
    }

    /**
     * Set numeroMaximoDeHorasPorSemana
     *
     * @param string $numeroMaximoDeHorasPorSemana
     * @return anadirEmpleadoGrado
     */
    public function setNumeroMaximoDeHorasPorSemana($numeroMaximoDeHorasPorSemana)
    {
        $this->numeroMaximoDeHorasPorSemana = $numeroMaximoDeHorasPorSemana;
    
        return $this;
    }

    /**
     * Get numeroMaximoDeHorasPorSemana
     *
     * @return string 
     */
    public function getNumeroMaximoDeHorasPorSemana()
    {
        return $this->numeroMaximoDeHorasPorSemana;
    }

    /**
     * Set estado
     *
     * @param boolean $estado
     * @return anadirEmpleadoGrado
     */
    public function setEstado($estado)
    {
        $this->estado = $estado;
    
        return $this;
    }

    /**
     * Get estado
     *
     * @return boolean 
     */
    public function getEstado()
    {
        return $this->estado;
    }
}