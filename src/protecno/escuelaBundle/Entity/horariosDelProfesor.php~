<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * horariosDelProfesor
 */
class horariosDelProfesor
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $descripcion;

    /**
     * @var string
     */
    private $nuevoHorario;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set descripcion
     *
     * @param string $descripcion
     * @return horariosDelProfesor
     */
    public function setDescripcion($descripcion)
    {
        $this->descripcion = $descripcion;
    
        return $this;
    }

    /**
     * Get descripcion
     *
     * @return string 
     */
    public function getDescripcion()
    {
        return $this->descripcion;
    }
    /**
     * Set nuevoHorario
     *
     * @param string $nuevoHorario
     * @return horariosDelProfesor
     */
    public function setNuevoHorario($nuevoHorario)
    {
        $this->nuevoHorario = $nuevoHorario;
    
        return $this;
    }

    /**
     * Get nuevoHorario
     *
     * @return string 
     */
    public function getNuevoHorario()
    {
        return $this->nuevoHorario;
    }
}