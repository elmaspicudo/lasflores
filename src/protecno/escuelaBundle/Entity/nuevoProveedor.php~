<?php

namespace protecno\escuelaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * nuevoProveedor
 */
class nuevoProveedor
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $tipoDeProveedor;

    /**
     * @var string
     */
    private $nombreDelProveedor;

    /**
     * @var integer
     */
    private $contactoNo;

    /**
     * @var string
     */
    private $direccion;

    /**
     * @var integer
     */
    private $tINNo;

    /**
     * @var string
     */
    private $region;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set tipoDeProveedor
     *
     * @param string $tipoDeProveedor
     * @return nuevoProveedor
     */
    public function setTipoDeProveedor($tipoDeProveedor)
    {
        $this->tipoDeProveedor = $tipoDeProveedor;
    
        return $this;
    }

    /**
     * Get tipoDeProveedor
     *
     * @return string 
     */
    public function getTipoDeProveedor()
    {
        return $this->tipoDeProveedor;
    }

    /**
     * Set nombreDelProveedor
     *
     * @param string $nombreDelProveedor
     * @return nuevoProveedor
     */
    public function setNombreDelProveedor($nombreDelProveedor)
    {
        $this->nombreDelProveedor = $nombreDelProveedor;
    
        return $this;
    }

    /**
     * Get nombreDelProveedor
     *
     * @return string 
     */
    public function getNombreDelProveedor()
    {
        return $this->nombreDelProveedor;
    }

    /**
     * Set contactoNo
     *
     * @param integer $contactoNo
     * @return nuevoProveedor
     */
    public function setContactoNo($contactoNo)
    {
        $this->contactoNo = $contactoNo;
    
        return $this;
    }

    /**
     * Get contactoNo
     *
     * @return integer 
     */
    public function getContactoNo()
    {
        return $this->contactoNo;
    }

    /**
     * Set direccion
     *
     * @param string $direccion
     * @return nuevoProveedor
     */
    public function setDireccion($direccion)
    {
        $this->direccion = $direccion;
    
        return $this;
    }

    /**
     * Get direccion
     *
     * @return string 
     */
    public function getDireccion()
    {
        return $this->direccion;
    }

    /**
     * Set tINNo
     *
     * @param integer $tINNo
     * @return nuevoProveedor
     */
    public function setTINNo($tINNo)
    {
        $this->tINNo = $tINNo;
    
        return $this;
    }

    /**
     * Get tINNo
     *
     * @return integer 
     */
    public function getTINNo()
    {
        return $this->tINNo;
    }

    /**
     * Set region
     *
     * @param string $region
     * @return nuevoProveedor
     */
    public function setRegion($region)
    {
        $this->region = $region;
    
        return $this;
    }

    /**
     * Get region
     *
     * @return string 
     */
    public function getRegion()
    {
        return $this->region;
    }
}